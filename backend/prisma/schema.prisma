datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}
model User {
    id                 String    @id @unique
    email              String    @unique
    username           String    @unique
    createdAt          DateTime?
    hashSalt           String?
    password           String
    refreshToken       String?
    refreshTokenIssued DateTime?
}
